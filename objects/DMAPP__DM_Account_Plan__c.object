    <fields>
        <fullName>DM_Overall_Completeness__c</fullName>
        <externalId>false</externalId>
        <formula>((DMAPP__LOR_Completion__c) + 
                (DMAPP__White_Space__c) + 
                (DMAPP__Segmentation_Completion__c) + 
                (DMAPP__Strategy_Map_Level__c) + 
                (DMAPP__Political_Map_Level__c) + 
                (DMAPP__Value_Map_Completion__c) + 
                (DMAPP__Objectives_Level__c) + 
                (DMAPP__Actions_Ratio__c) + 
                (DMAPP__Plan_Details_Attainment__c)) /9</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Overall Completeness</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
