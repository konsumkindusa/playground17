    <validationRules>
        <fullName>Not_Allowed_Change_Record_Type</fullName>
        <active>true</active>
        <description>User cannot change Territory rule criteria record type when it has rule criteria values associated with it</description>
        <errorConditionFormula>AND(PRIORVALUE( RecordTypeId )&lt;&gt; RecordTypeId,  Values__c &gt;0)</errorConditionFormula>
        <errorMessage>You cannot change Rule Criteria Record Type when it has criteria values associated with it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Score_Value_Should_be_in_Range_Of_1to200</fullName>
        <active>true</active>
        <description>Prevent Score value to get exceed by 200.</description>
        <errorConditionFormula>NOT(AND(Score__c &gt;= 1, Score__c &lt;= 200))</errorConditionFormula>
        <errorDisplayField>Score__c</errorDisplayField>
        <errorMessage>Score value should be in between 1-200.</errorMessage>
    </validationRules>
