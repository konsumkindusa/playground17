    <validationRules>
        <fullName>Do_Not_Allow_Multiple_Primary_Accounts</fullName>
        <active>false</active>
        <description>Only one Account can be marked as a primary on the project when multiple accounts associated with project</description>
        <errorConditionFormula>AND(ISCHANGED( is_Primary__c ),is_Primary__c = true, Project__r.Primary_Number__c =1, Project__r.Count_Account__c &gt;1, PRIORVALUE(is_Primary__c)=false)</errorConditionFormula>
        <errorDisplayField>is_Primary__c</errorDisplayField>
        <errorMessage>Error: Only one Account can be set as Primary</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Primary_Account_Required</fullName>
        <active>false</active>
        <description>Primary Account required for Territory Support</description>
        <errorConditionFormula>AND( Project__r.Count_Account__c =1,ISPICKVAL(Project__r.Purpose__c ,&quot;Territory Support&quot;),ISCHANGED( is_Primary__c ),is_Primary__c =false)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Territory Support project must have a Primary Account</errorMessage>
    </validationRules>
