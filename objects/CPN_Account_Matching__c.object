    <validationRules>
        <fullName>Parent_Account_Has_CPN_Flag</fullName>
        <active>true</active>
        <description>Account can&apos;t be set to Parent if it&apos;s not marked as CPN</description>
        <errorConditionFormula>AND(NOT(Parent_Account__r.Is_Corporate_Parent__c ), ISPICKVAL(Match_Status__c,&apos;Confirmed&apos;))</errorConditionFormula>
        <errorMessage>Parent Account must be CPN</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Update_Match_Status</fullName>
        <active>true</active>
        <description>Users can&apos;t update match status for confirmed records</description>
        <errorConditionFormula>AND(ISCHANGED( Match_Status__c ),  ISPICKVAL(PRIORVALUE(Match_Status__c),&apos;Confirmed&apos;))</errorConditionFormula>
        <errorDisplayField>Match_Status__c</errorDisplayField>
        <errorMessage>You cannot update status of the Confirmed records</errorMessage>
    </validationRules>
