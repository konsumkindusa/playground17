    <validationRules>
        <fullName>Cannot_Update_an_existing_License_ID</fullName>
        <active>true</active>
        <description>If an ETA has a License ID associated to it, this rule will prevent them from updating the License ID. This rule is implemented for MAOTS integrations.</description>
        <errorConditionFormula>PRIORVALUE(License_ID__c) &lt;&gt; Null &amp;&amp; ISCHANGED(License_ID__c)</errorConditionFormula>
        <errorDisplayField>License_ID__c</errorDisplayField>
        <errorMessage>License ID on an ETA cannot be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckLicenseBeforeEnablingLEUCamp</fullName>
        <active>true</active>
        <description>If the &quot;Enable LEU Campaigns&quot; field is checked without the License ID field being populated. 
The error message should say &quot;Please provide a License ID before enabling LEU campaigns.&quot;</description>
        <errorConditionFormula>AND(Enable_LEU_Campaigns__c=true, License_ID__c=null)</errorConditionFormula>
        <errorDisplayField>License_ID__c</errorDisplayField>
        <errorMessage>Please provide a License ID before enabling LEU campaigns.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Invalid_Term_Dates</fullName>
        <active>true</active>
        <description>Prevent Invalid Term Dates on Enterprise Training Agreements</description>
        <errorConditionFormula>Term_Start_Date__c  &gt;=  Term_End_Date__c</errorConditionFormula>
        <errorDisplayField>Term_End_Date__c</errorDisplayField>
        <errorMessage>Term End Date must be greater than Term Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RestrictRenewalDateOnCreatingBefore48Hrs</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISCHANGED(Renewal_Notification_Date__c), Renewal_Notification_Date__c &lt;  (NOW()+(0.04167*48)))</errorConditionFormula>
        <errorDisplayField>Renewal_Notification_Date__c</errorDisplayField>
        <errorMessage>Date should be 48 Hrs from Todays Date.</errorMessage>
    </validationRules>
