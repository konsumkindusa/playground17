    <validationRules>
        <fullName>Score_Value_Should_be_in_Range_Of_1to200</fullName>
        <active>true</active>
        <description>Prevent Score value to get exceed by 200.</description>
        <errorConditionFormula>IF(NOT(ISBLANK(Score__c)),NOT(AND(Score__c &gt;= 1, Score__c &lt;= 200)),False)</errorConditionFormula>
        <errorDisplayField>Score__c</errorDisplayField>
        <errorMessage>Score value should be in between 1-200.</errorMessage>
    </validationRules>
