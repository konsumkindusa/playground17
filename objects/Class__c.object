    <fields>
        <fullName>Commerce_Orderable__c</fullName>
        <description>Commerce orderable should be set to true if Class and Location and Course are all active.</description>
        <externalId>false</externalId>
        <formula>AND(
	OR(
		ISPICKVAL(Status__c , &apos;Active&apos;)
		,ISPICKVAL(Status__c , &apos;Completed&apos;)
	)
	,ISPICKVAL(Class_Type__c , &apos;Public&apos;)        
	,ISPICKVAL(Course__r.Status__c , &apos;Active&apos;)
	,OR(
		OR( 
			ISPICKVAL(Class_Format__c, &apos;Instructor Led Online&apos;)
			,ISPICKVAL(Class_Format__c, &apos;Self-Paced Online&apos;)	 
		)
		,AND(
			OR( 
				ISPICKVAL(Class_Format__c, &apos;Instructor Led Classroom&apos;)
				,ISPICKVAL(Class_Format__c, &apos;Certification&apos;) 
			)
			,ISPICKVAL(Training_Location__r.Status__c, &apos;Active&apos;)
		)
	)
)</formula>
        <label>Commerce Orderable</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
