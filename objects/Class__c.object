    <validationRules>
        <fullName>Certification_Exam_Proctor_Rule</fullName>
        <active>true</active>
        <description>A Certification Exam cannot be marked as Completed if there is no Proctor assigned.</description>
        <errorConditionFormula>ISCHANGED( Status__c ) &amp;&amp;  ISBLANK( Proctor__c ) &amp;&amp;  ISPICKVAL( Class_Format__c , &apos;Certification&apos;) &amp;&amp; ISPICKVAL(Status__c, &apos;Completed&apos;)</errorConditionFormula>
        <errorMessage>Certification Exam cannot be marked Completed without a Proctor assigned.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Class_Activate_Rule</fullName>
        <active>true</active>
        <description>Allow class to be activated if the course and location are active. Do not allow to</description>
        <errorConditionFormula>AND 
(
  ISPICKVAL(Status__c, &quot;Active&quot;)
 , OR
  (
     NOT(ISPICKVAL(Course__r.Status__c, &quot;Active&quot;))  
    ,AND( 
        OR(
             $RecordType.Name = &quot;Instructor Led&quot;
            , $RecordType.Name = &quot;Exam&quot;
          ) 
      , OR(
             ISBLANK(Training_Location__c) 
            , NOT(ISPICKVAL(Training_Location__r.Status__c, &quot;Active&quot;))
          ) 
        )    
  )
, not($Permission.Allow_Training_Data_Changes) 
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Associated Course and Location (if applicable) should be active before setting Class Status to Active</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Class_Complete_Rule</fullName>
        <active>true</active>
        <description>Do not allow the Class to marked as Complete if Total Seats taken is 0 and there is no Primary Instructor assigned.</description>
        <errorConditionFormula>AND( (ISCHANGED(Status__c)),(ISPICKVAL(Status__c, &apos;Completed&apos;)), (Total_Seats_Taken__c = 0))</errorConditionFormula>
        <errorMessage>Class cannot be marked as Completed as Total Seats Taken are 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Class_Date_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>OR( End_Date__c &lt;  Start_Date__c)</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Class End Date should be greater than Class Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Class_Max_Seats_Greater_Total_Seats</fullName>
        <active>true</active>
        <errorConditionFormula>AND
   (
	Max_Seats__c &gt;= 0  
	,Max_Seats__c &lt; Total_Seats_Taken__c 
	,NOT(RecordType.DeveloperName = &quot;Self_Paced&quot;)
   )</errorConditionFormula>
        <errorMessage>Cannot register more enrollees as Class is already full</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Class_Primary_Instructor_Rule</fullName>
        <active>true</active>
        <description>Class cannot be marked as Completed as there is no Primary Instructor assigned.</description>
        <errorConditionFormula>AND( ISCHANGED(Status__c),  ISPICKVAL(Status__c, &apos;Completed&apos;),  ISBLANK( Primary_Instructor__c ), OR( ISPICKVAL( Class_Format__c , &apos;Instructor Led Classroom&apos;) , ISPICKVAL( Class_Format__c, &apos;Instructor Led Online&apos;)) )</errorConditionFormula>
        <errorMessage>Class cannot be marked Completed without a Primary Instructor assigned.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Class_Publish_To_Web_Rule</fullName>
        <active>true</active>
        <description>Do not allow users to Publish Class to Web if the associated Course and Location are not Published to web</description>
        <errorConditionFormula>AND 
(
   NOT(ISNEW())  
 , Publish_To_Web__c = TRUE
 , OR
  (
     Course__r.Publish_to_Web__c = FALSE   
    ,AND( 
        OR(
             $RecordType.Name = &quot;Instructor Led&quot;
            , $RecordType.Name = &quot;Exam&quot;
          ) 
      , OR(
             ISBLANK(Training_Location__c) 
            , Training_Location__r.Publish_To_Web__c = FALSE  
          ) 
        )    
  )
, not($Permission.Allow_Training_Data_Changes) 
)</errorConditionFormula>
        <errorDisplayField>Publish_To_Web__c</errorDisplayField>
        <errorMessage>Associated Course and Location (if applicable) should be published before publishing the class</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Class_Type</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
	ISPICKVAL( Class_Type__c, &quot;&quot;)  
	,ISPICKVAL(Status__c , &quot;Active&quot;)
        , not($Permission.Allow_Training_Data_Changes)
)</errorConditionFormula>
        <errorDisplayField>Class_Type__c</errorDisplayField>
        <errorMessage>Class Type is required when Status is Active</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Class_creation_rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
	NOT(ISPICKVAL( Course__r.Status__c , &quot;Active&quot;)), 
	ISNEW() , 
        not($Permission.Allow_Training_Data_Changes)
)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Cannot create new class if Course is not Active</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Status_for_Class_with_Enrollment</fullName>
        <active>true</active>
        <description>Setting the status to &apos;Mark for Deletion&apos; is invalid if the class has enrollments and is published to the web.</description>
        <errorConditionFormula>AND (
      OR (ISCHANGED(Status__c),ISCHANGED(Publish_To_Web__c)), 
      ISPICKVAL( Status__c , &apos;Mark for Deletion&apos;), 
       OR (
           Publish_To_Web__c = true
           ,ActualCountofEnrollments__c &gt; 0
          )
    )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Cannot set status to Mark for Deletion when class has Enrollments or Class is Published to the Web.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Max_Seats_Zero</fullName>
        <active>true</active>
        <description>Validation rule to check that max seats are &gt;=0</description>
        <errorConditionFormula>AND 
( 
	NOT(RecordType.DeveloperName  = &quot;Self-Paced&quot;)
	,Max_Seats__c &lt;0
)</errorConditionFormula>
        <errorDisplayField>Max_Seats__c</errorDisplayField>
        <errorMessage>Max Seats should be greater than or equal to 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Read_Only_Fields_ClassStatus</fullName>
        <active>true</active>
        <errorConditionFormula>AND 
( 
	NOT(ISCHANGED(Status__c))
	,OR
	(
	 ISPICKVAL(Status__c, &quot;Completed&quot;)
	)
	,OR 
	(	 
	   PRIORVALUE( Name ) &lt;&gt; Name
	  ,PRIORVALUE(Default_Duration__c) &lt;&gt; Default_Duration__c 
	  ,PRIORVALUE(End_Date__c) &lt;&gt;  End_Date__c 
	  ,PRIORVALUE(End_Time__c) &lt;&gt; End_Time__c 
	  ,PRIORVALUE(Start_Date__c) &lt;&gt; Start_Date__c
	  ,PRIORVALUE(Start_Time__c) &lt;&gt; Start_Time__c 
	  ,PRIORVALUE( Language__c ) &lt;&gt; Language__c
	  ,PRIORVALUE(Publish_To_Web__c ) &lt;&gt; Publish_To_Web__c
	  ,PRIORVALUE(Sponsor_Office__c) &lt;&gt; Sponsor_Office__c
	  ,PRIORVALUE(Max_Seats__c) &lt;&gt; Max_Seats__c 
	  ,PRIORVALUE(Send_Email__c) &lt;&gt; Send_Email__c
	  ,PRIORVALUE(Training_Location__c) &lt;&gt;  Training_Location__c
	),
        not($Permission.Allow_Training_Data_Changes) 
)</errorConditionFormula>
        <errorMessage>Cannot change Class Name, Default Duration, Start Date, Start Time, End Date , End Time, Sponsor Office, Location, Language, Max Seats, Publish to Web, Send Email when Status is Active or Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Seat_Price</fullName>
        <active>true</active>
        <errorConditionFormula>OR 
( AND
  (
     ISPICKVAL( Status__c , &quot;Active&quot;)
    ,ISPICKVAL(Class_Type__c, &quot;Public&quot;)
    ,Publish_To_Web__c = True
    ,not($Permission.Allow_Training_Data_Changes)
    ,OR (ISBLANK( Seat_Price__c), Seat_Price__c  &lt;= 0)
  ) 
  , Seat_Price__c  &lt; 0
 )</errorConditionFormula>
        <errorDisplayField>Seat_Price__c</errorDisplayField>
        <errorMessage>Please enter valid amount in Seat Price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Time_Format_Validation_Rule_End_Time</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT ISBLANK(  End_Time__c ) ,
NOT(REGEX(  End_Time__c , &apos;^((([0][0-9]|[1][0-1])|[0-9]):([0-5][0-9])( *)(AM))|((([0][0-9]|[1][0-2])|[0-9]):([0-5][0-9])( *)(PM))$&apos;)))</errorConditionFormula>
        <errorDisplayField>End_Time__c</errorDisplayField>
        <errorMessage>Must be HH:MM AM/PM format and be a valid time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Time_Format_Validation_Rule_Start_Time</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT ISBLANK(  Start_Time__c ) ,
NOT(REGEX(  Start_Time__c , &apos;^((([0][0-9]|[1][0-1])|[0-9]):([0-5][0-9])( *)(AM))|((([0][0-9]|[1][0-2])|[0-9]):([0-5][0-9])( *)(PM))$&apos;)))</errorConditionFormula>
        <errorDisplayField>Start_Time__c</errorDisplayField>
        <errorMessage>Must be HH:MM AM/PM format and be a valid time</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
