    <fields>
        <fullName>Webstore_Orderable__c</fullName>
        <description>Webstore orderable flag should be set to true if Class and location and course are active and published to web</description>
        <externalId>false</externalId>
        <formula>AND(
	ISPICKVAL(Status__c , &apos;Active&apos;)
	,ISPICKVAL(Class_Type__c , &apos;Public&apos;)        
	,ISPICKVAL(Course__r.Status__c , &apos;Active&apos;)
	,Course__r.Publish_to_Web__c
	,Publish_To_Web__c
	,OR(
		OR( 
			ISPICKVAL(Class_Format__c, &apos;Instructor Led Online&apos;)
			,ISPICKVAL(Class_Format__c, &apos;Self-Paced Online&apos;)	 
		)
		,AND(
			OR( 
				ISPICKVAL(Class_Format__c, &apos;Instructor Led Classroom&apos;)
				,ISPICKVAL(Class_Format__c, &apos;Certification&apos;) 
			)
			,ISPICKVAL(Training_Location__r.Status__c, &apos;Active&apos;)
			,Training_Location__r.Publish_To_Web__c
		)
	)
)</formula>
        <label>Webstore Orderable</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
