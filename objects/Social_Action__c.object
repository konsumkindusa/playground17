    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Amount_Raised_by_Staff_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
(text( Status__c ) = &quot;Completed&quot;),
(text(Category__c ) = &quot;Global Relief&quot;), 
(text(SubCategory__c ) = &quot;Disaster Relief (staff matching)&quot;),
(Text(Staff_Monetary_Contribution__c ) = &quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>Staff_Monetary_Contribution__c</errorDisplayField>
        <errorMessage>Staff Monetary Contribution is required to Complete or Approve the record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Currency_Required_when_Award_is_Closed</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
(text(  Status__c  ) = &quot;Completed&quot;),
(text( Currency__c )=&quot;&quot;),
( RecordType.DeveloperName ) = &quot;Awards&quot;
)</errorConditionFormula>
        <errorDisplayField>Currency__c</errorDisplayField>
        <errorMessage>Currency is required to Complete  the record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_award_end_date</fullName>
        <active>true</active>
        <description>requires that an end date is is required when an the Social Action is complete.</description>
        <errorConditionFormula>AND( ISPICKVAL(Status__c, &quot;Completed&quot;),
NOT(text( Type__c ) = &quot;General Inquiry&quot;),
ISBLANK( End_Date__c )
)</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End Date is Required to Complete a Social Action</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Score_Required</fullName>
        <active>true</active>
        <description>Scor is required when 	Record=Award, Category= Community Grant, Status = Completed</description>
        <errorConditionFormula>AND ( 
RecordType.DeveloperName = &quot;Awards&quot;,
(text( Category__c ) = &quot;Community Grant&quot;),
(text( Status__c ) = &quot;Completed&quot;),
( Score__c ) = &quot;&quot;
)</errorConditionFormula>
        <errorDisplayField>Score__c</errorDisplayField>
        <errorMessage>Score required when Record=Award, Category= Community Grant, and Status = Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Contribution_required_to_Complete</fullName>
        <active>true</active>
        <description>total contribution required to Complete an Award</description>
        <errorConditionFormula>AND( 
OR((text( Status__c ) = &quot;Completed&quot;),(text( Status__c ) = &quot;Approved&quot;)), 
((  Total_Monetary_Contribution__c  )&lt;1), 
(RecordType.Name ) = &quot;Awards&quot; 
)</errorConditionFormula>
        <errorMessage>Total Monetary Contribution Required when Status = Completed or Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>US_Tax_Documentation_received_required</fullName>
        <active>true</active>
        <description>US Tax Documentation received is required when Type=Awards, and Category= Community Grants, A-Thon, and MathWorks Office=[all US coded records, Natick, Novi, LA, etc.], and Status= Complete</description>
        <errorConditionFormula>And(
 ISPICKVAL(Type__c, &quot;Awards&quot;), 
OR(ISPICKVAL( Category__c, &quot;Community Grants&quot;), ISPICKVAL( Category__c, &quot;A-thon&quot;)), 
OR (ISPICKVAL( MathWorks_Office__c, &quot;Natick&quot;), ISPICKVAL( MathWorks_Office__c, &quot;US - Natick&quot;), ISPICKVAL( MathWorks_Office__c, &quot;US - Novi&quot;), ISPICKVAL( MathWorks_Office__c, &quot;US - All&quot;), ISPICKVAL( MathWorks_Office__c, &quot;US - All&quot;)), 
ISPICKVAL( Status__c, &quot;Completed&quot;), 
 US_Tax_Documentation_Recieved__c = FALSE
)</errorConditionFormula>
        <errorDisplayField>US_Tax_Documentation_Recieved__c</errorDisplayField>
        <errorMessage>US Tax Documentation received is required to complete the Social Action</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>can_not_complete_with_Queue_as_owner</fullName>
        <active>true</active>
        <description>do not allow Users to Complete a Social Action with the Queue as the Owner</description>
        <errorConditionFormula>AND (
 text(Status__c ) = &quot;Completed&quot;,
 NOT(ISBLANK(Owner:Queue.Id))
)</errorConditionFormula>
        <errorDisplayField>OwnerId</errorDisplayField>
        <errorMessage>Owner can not be a queue</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
