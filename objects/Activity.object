    <fields>
        <fullName>Days_Overdue__c</fullName>
        <description>For open tasks, calculates the number of days between today and due date. If number is &lt;= 0 returns a blank value. If task due date is null, returns null</description>
        <externalId>false</externalId>
        <formula>IF(IsClosed,NULL,IF((TODAY()-ActivityDate &gt; 0), TODAY()-ActivityDate,NULL))</formula>
        <label>Days Overdue</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Days_between_Created_and_Closed__c</fullName>
        <externalId>false</externalId>
        <formula>IF(IsClosed , Close_Date_Time__c  -  Created_Date_Time__c ,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Days between Created and Closed</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
