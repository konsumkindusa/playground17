    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <enableFeeds>false</enableFeeds>
    <fieldSets>
        <fullName>OpportunityProduct</fullName>
        <description>This will be used on Quote New Override page</description>
        <displayedFields>
            <field>Product2.Name</field>
            <isFieldManaged>false</isFieldManaged>
            <isRequired>false</isRequired>
        </displayedFields>
        <displayedFields>
            <field>ProductCode</field>
            <isFieldManaged>false</isFieldManaged>
            <isRequired>false</isRequired>
        </displayedFields>
        <displayedFields>
            <field>Quantity</field>
            <isFieldManaged>false</isFieldManaged>
            <isRequired>false</isRequired>
        </displayedFields>
        <label>OpportunityProduct</label>
    </fieldSets>
    <fields>
        <fullName>Description</fullName>
    </fields>
    <fields>
        <fullName>Discount</fullName>
    </fields>
    <fields>
        <fullName>ListPrice</fullName>
    </fields>
    <fields>
        <fullName>OpportunityId</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Product2Id</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>ProductCode</fullName>
    </fields>
    <fields>
        <fullName>Product_Topic__c</fullName>
        <externalId>false</externalId>
        <formula>PricebookEntry.Product2.Topic__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Product Topic</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Quantity</fullName>
    </fields>
    <fields>
        <fullName>Revenue_Class__c</fullName>
        <description>Used to assign a revenue class to an opportunity product (line item).  This field is used in roll up summary fields on the opportunity to understand revenue class attribution.</description>
        <externalId>false</externalId>
        <label>Revenue Class</label>
        <required>false</required>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Perpetual</fullName>
                    <default>true</default>
                    <label>Perpetual</label>
                </value>
                <value>
                    <fullName>Term New</fullName>
                    <default>false</default>
                    <label>Term New</label>
                </value>
                <value>
                    <fullName>Term Renewal</fullName>
                    <default>false</default>
                    <label>Term Renewal</label>
                </value>
                <value>
                    <fullName>SMS</fullName>
                    <default>false</default>
                    <label>SMS</label>
                </value>
                <value>
                    <fullName>Training</fullName>
                    <default>false</default>
                    <label>Training</label>
                </value>
                <value>
                    <fullName>Consulting</fullName>
                    <default>false</default>
                    <label>Consulting</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>ServiceDate</fullName>
    </fields>
    <fields>
        <fullName>Subtotal</fullName>
    </fields>
    <fields>
        <fullName>TotalPrice</fullName>
    </fields>
    <fields>
        <fullName>UnitPrice</fullName>
    </fields>
    <validationRules>
        <fullName>Revenue_Class_Must_be_Specified</fullName>
        <active>true</active>
        <description>Revenue Class Must be Specified for Sales Products</description>
        <errorConditionFormula>AND( Product2.Sales_Product__c ,  TEXT(Product2.Family)  &lt;&gt; &quot;Unclassified&quot; , ISBLANK( TEXT(Revenue_Class__c )) )</errorConditionFormula>
        <errorDisplayField>Revenue_Class__c</errorDisplayField>
        <errorMessage>Revenue Class Must be Specified for Sales Products</errorMessage>
    </validationRules>
</CustomObject>
