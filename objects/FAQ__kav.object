    <validationRules>
        <fullName>Geck_ID_is_less_than_8_numbers</fullName>
        <active>true</active>
        <description>Geck ID can not be greater that 8 numbers in length</description>
        <errorConditionFormula>AND(NOT(ISBLANK(GeckId__c)),OR( 
LEN (GeckId__c) &gt;8, 
NOT(ISNUMBER( GeckId__c )) 
))</errorConditionFormula>
        <errorMessage>Geck ID must be a numeric value of 8 numbers or less. Only one geck ID is allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Major_Release_Required</fullName>
        <active>true</active>
        <description>the Major release field is always required on save</description>
        <errorConditionFormula>isblank(text( Major_Release__c ))</errorConditionFormula>
        <errorMessage>Major Release is required to save</errorMessage>
    </validationRules>
