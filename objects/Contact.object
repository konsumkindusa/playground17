NOT(OR(OR( LEN(Last_Name_Latin__c) = 0,REGEX(    
Last_Name_Latin__c , 
$Setup.General_Settings__c.RegEx_Latin__c )))),
NOT(OR(OR( LEN( Mail_Stop_Latin__c ) = 0,REGEX(      Mail_Stop_Latin__c ,  
$Setup.General_Settings__c.RegEx_Latin__c )))),
NOT(OR(OR( LEN( First_Name_Latin__c  ) = 0,REGEX(      First_Name_Latin__c,  
