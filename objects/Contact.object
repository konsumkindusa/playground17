    <validationRules>
        <fullName>Change_Disable_Merge_to_Dup</fullName>
        <active>true</active>
        <description>Cannot change to Dup if DisableMerge flag is ON</description>
        <errorConditionFormula>AND( NOT( ISBLANK( Kept_Contact_Name__c ) ) , Disable_Merge__c = True)</errorConditionFormula>
        <errorDisplayField>Kept_Contact_Name__c</errorDisplayField>
        <errorMessage>You cannot change status to Duplicate if Disable Merge flag is ON</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Change_Status_to_Duplicate</fullName>
        <active>true</active>
        <description>Users cannot manulay change status to Duplicate, only when Kept Contact name has been populated</description>
        <errorConditionFormula>AND ( ISCHANGED( Contact_Status__c) , ISPICKVAL( Contact_Status__c , &quot;Duplicate&quot;), ISBLANK( Kept_Contact_Name__c ))</errorConditionFormula>
        <errorDisplayField>Contact_Status__c</errorDisplayField>
        <errorMessage>To change contact status to Duplicate you must enter the value of Kept Contact Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ConStatus_From_To_Duplicate_Restriction</fullName>
        <active>true</active>
        <description>Contact status cannot be manually set to or from the &quot;Duplicate&quot; status. Only Admins can do it.</description>
        <errorConditionFormula>OR(
   AND(
   ISPICKVAL(PRIORVALUE( Contact_Status__c), &apos;Duplicate&apos;),
   ISCHANGED(Contact_Status__c), $Setup.Contact_Validation_Rule__c.Prevent_Setting_Status_Duplicate__c 
   ),
   AND(
     ISPICKVAL(Contact_Status__c, &apos;Duplicate&apos;),
     ISCHANGED(Contact_Status__c), $Setup.Contact_Validation_Rule__c.Prevent_Setting_Status_Duplicate__c  
   )
)</errorConditionFormula>
        <errorDisplayField>Contact_Status__c</errorDisplayField>
        <errorMessage>Contact status cannot be manually set to or from the &quot;Duplicate&quot; status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_Is_Licensed_Validate_Status_Res</fullName>
        <active>true</active>
        <description>If the contact is licensed, only a Data Steward can set the contact status from other than Duplicate.</description>
        <errorConditionFormula>AND(
 Has_Licenses__c = true,
 NOT( $Permission.Allow_Licensed_Contact_Status_Update),
 ISCHANGED( Contact_Status__c ),
 ISPICKVAL(Contact_Status__c, &apos;Duplicate&apos;)
)</errorConditionFormula>
        <errorDisplayField>Contact_Status__c</errorDisplayField>
        <errorMessage>Contact is licensed, Only a Data Administrator can set the contact status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email_Is_Required</fullName>
        <active>true</active>
        <description>Email is required when profile is not system admin and integration user</description>
