    <webLinks>
        <fullName>Sync_Lead</fullName>
        <availability>online</availability>
        <description>It will sync information from the contact to OPEN lead record where the salesforce contact id is equal to the contact id that the button was pushed.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Sync Lead</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>/** 
* @author Original: Archana Lohar- Cloud Sherpas/Mathworks, Last Modified: Archana Lohar - Cloud Sherpas/Mathworks 
* @date Original: 5 Mar 2015, Last Modified: 5 Mar 2015 
*/ 

 //adds the proper code for inclusion of AJAX toolkit 
 {!REQUIRESCRIPT(&quot;/soap/ajax/13.0/connection.js&quot;)} 
 {!REQUIRESCRIPT(&quot;/support/console/27.0/integration.js&quot;)}

//for showing loading Image 
var errorDiv = document.getElementById(&apos;errorDiv_ep&apos;); 

errorDiv.innerHTML = &quot;&lt;img class=&apos;waitingImage&apos; src=&apos;/img/loading.gif&apos; title=&apos;Please Wait...&apos; /&gt; &lt;span class=&apos;waitingDescription&apos;&gt;Sync In Progress...&lt;/span&gt;&quot;; 

errorDiv.style.display = &quot;block&quot;; 


 var leadRecords= sforce.connection.query(&quot;SELECT id  FROM Lead  WHERE Contact__c =    &apos;{!Contact.Id}&apos; AND status!=&apos;Closed - Converted&apos; AND  status!=&apos;Closed - Unconverted&apos;&quot;);


 var lRecords= leadRecords.getArray(&quot;records&quot;);

 var updateRecords = [];

 //Iterate through the returned results 
 for (var LeadIndex=0;LeadIndex&lt;lRecords.length;LeadIndex++) { 
   
      var LeadObj = lRecords[LeadIndex]; 

      var LeadUpdate = new sforce.SObject(&quot;Lead&quot;); 
      LeadUpdate.Id = LeadObj.Id;

      if(&apos;{!Contact.Salutation}&apos;.length!=0) {
       LeadUpdate.Salutation=&apos;{!Contact.Salutation}&apos;;
      }
      if(&apos;{!Contact.FirstName}&apos;.length!=0) {
       LeadUpdate.FirstName=&apos;{!Contact.FirstName}&apos;;
      }
      if(&apos;{!Contact.LastName}&apos;.length!=0) {
       LeadUpdate.LastName=&apos;{!Contact.LastName}&apos;;
      }
      if(&apos;{!Contact.Title}&apos;.length!=0) {
       LeadUpdate.Title=&apos;{!Contact.Title}&apos;;
      }
      if(&apos;{!Contact.Job_Role__c}&apos;.length!=0) {
       LeadUpdate.Job_Role__c=&apos;{!Contact.Job_Role__c}&apos;;
      }
      if(&apos;{!Contact.MailingCountry}&apos;.length!=0) {
       LeadUpdate.country=&apos;{!Contact.MailingCountry}&apos;;
      }
      //Updated by Haigang Wu, for Sprint 2016.5.1, story 1429194
      if(&apos;{!Contact.Country_State_Code__c}&apos;.length!=0) {
       LeadUpdate.Country_State_Code__c=&apos;{!Contact.Country_State_Code__c}&apos;;
      }
      if(&apos;{!JSENCODE(Contact.MailingStreet)}&apos;.length!=0) {
       LeadUpdate.Street=&apos;{!JSENCODE(Contact.MailingStreet)}&apos;;
      }
      if(&apos;{!Contact.MailingCity}&apos;.length!=0) {
       LeadUpdate.City=&apos;{!Contact.MailingCity}&apos;;
      }
      if(&apos;{!Contact.MailingState}&apos;.length!=0) {
       LeadUpdate.State=&apos;{!Contact.MailingState}&apos;;
      }
      //if(&apos;{!Contact.MailingStateCode}&apos;.length!=0) {
      // LeadUpdate.StateCode=&apos;{!Contact.MailingStateCode}&apos;;
      //}
      if(&apos;{!Contact.MailingPostalCode}&apos;.length!=0) {
       LeadUpdate.PostalCode=&apos;{!Contact.MailingPostalCode}&apos;;
      }
      
      if(&apos;{!Contact.Phone}&apos;.length!=0) {
       LeadUpdate.Phone=&apos;{!Contact.Phone}&apos;;
      }
      if(&apos;{!Contact.MobilePhone}&apos;.length!=0) {
       LeadUpdate.MobilePhone=&apos;{!Contact.MobilePhone}&apos;;
      }

      if(&apos;{!Contact.Fax}&apos;.length!=0) {
      LeadUpdate.Fax=&apos;{!Contact.Fax}&apos;;
      }
      
      if(&apos;{!Contact.Email}&apos;.length!=0) {
        LeadUpdate.Email=&apos;{!Contact.Email}&apos;;
      }
      
       if(&apos;{!Contact.DoNotCall}&apos;.length!=0) {
      LeadUpdate.DoNotCall=&apos;{!Contact.DoNotCall}&apos;;
      }
      
       if(&apos;{!Contact.Department}&apos;.length!=0) {
         LeadUpdate.Department__c=&apos;{!Contact.Department}&apos;;
      }
       if(&apos;{!Contact.First_Name_Latin__c}&apos;.length!=0) {
        LeadUpdate.First_Name_Latin__c=&apos;{!Contact.First_Name_Latin__c}&apos;;
      }
       if(&apos;{!Contact.Last_Name_Latin__c}&apos;.length!=0) {
        LeadUpdate.Last_Name_Latin__c=&apos;{!Contact.Last_Name_Latin__c}&apos;;
      }
       if(&apos;{!Contact.Job_Title_Latin__c}&apos;.length!=0) {
          LeadUpdate.Title_Latin__c=&apos;{!Contact.Job_Title_Latin__c}&apos;;
      }
      if(&apos;{!Contact.Department_Latin__c}&apos;.length!=0) {
           LeadUpdate.Department_Latin__c=&apos;{!Contact.Department_Latin__c}&apos;;
      }
      if(&apos;{!Contact.City_Latin__c}&apos;.length!=0) {
           LeadUpdate.City_Latin__c=&apos;{!Contact.City_Latin__c}&apos;;
      }
      if(&apos;{!Contact.State_Province_Latin__c}&apos;.length!=0) {
            LeadUpdate.State_Province_Latin__c=&apos;{!Contact.State_Province_Latin__c}&apos;;
      }
      if(&apos;{!Contact.Field_of_Study__c}&apos;.length!=0) {
            LeadUpdate.Field_of_Study__c=&apos;{!Contact.Field_of_Study__c}&apos;;
      }
   
      LeadUpdate.Account__C=&apos;{!Account.Id}&apos;;
      
      updateRecords.push(LeadUpdate); 
  }

try{

if (updateRecords.length&gt;0) { 

   sforce.connection.update(updateRecords);
    
 } 
}
catch(e){
  alert(e.toString());
}
//Refresh page 
   location.reload(true);</url>
    </webLinks>
