    <fields>
        <fullName>Certification_Training_Dates__c</fullName>
        <description>This formula field use to calculate the certification training dates and which is used in visualforce Email template</description>
        <externalId>false</externalId>
        <formula>IF( Start_Date__c == End_Date__c , 
text(DAY( Start_Date__c )) +&apos; &apos;+ CASE( MONTH( Start_Date__c ) , 
1, &quot;Jan&quot;, 
2, &quot;Feb&quot;, 
3, &quot;Mar&quot;, 
4, &quot;Apr&quot;, 
5, &quot;May&quot;, 
6, &quot;Jun&quot;, 
7, &quot;Jul&quot;, 
8, &quot;Aug&quot;, 
9, &quot;Sep&quot;, 
10, &quot;Oct&quot;, 
11, &quot;Nov&quot;, 
&quot;Dec&quot;) +&apos; &apos;+ Text(YEAR( Start_Date__c )) , 
IF( MONTH( Start_Date__c ) == MONTH( End_Date__c ), 
TEXT(DAY(Start_Date__c))+&apos; - &apos; 
+ 
text(DAY( End_Date__c )) +&apos; &apos;+ CASE( MONTH( End_Date__c ) , 
1, &quot;Jan&quot;, 
2, &quot;Feb&quot;, 
3, &quot;Mar&quot;, 
4, &quot;Apr&quot;, 
5, &quot;May&quot;, 
6, &quot;Jun&quot;, 
7, &quot;Jul&quot;, 
8, &quot;Aug&quot;, 
9, &quot;Sep&quot;, 
10, &quot;Oct&quot;, 
11, &quot;Nov&quot;, 
&quot;Dec&quot;) +&apos; &apos;+ Text(YEAR( End_Date__c )) 
,text(DAY( Start_Date__c )) +&apos; &apos;+ CASE( MONTH( Start_Date__c ) , 
1, &quot;Jan&quot;, 
2, &quot;Feb&quot;, 
3, &quot;Mar&quot;, 
4, &quot;Apr&quot;, 
5, &quot;May&quot;, 
6, &quot;Jun&quot;, 
7, &quot;Jul&quot;, 
8, &quot;Aug&quot;, 
9, &quot;Sep&quot;, 
10, &quot;Oct&quot;, 
11, &quot;Nov&quot;, 
&quot;Dec&quot;)+&apos; - &apos;+text(DAY( End_Date__c )) +&apos; &apos;+ CASE( MONTH( End_Date__c ) , 
1, &quot;Jan&quot;, 
2, &quot;Feb&quot;, 
3, &quot;Mar&quot;, 
4, &quot;Apr&quot;, 
5, &quot;May&quot;, 
6, &quot;Jun&quot;, 
7, &quot;Jul&quot;, 
8, &quot;Aug&quot;, 
9, &quot;Sep&quot;, 
10, &quot;Oct&quot;, 
11, &quot;Nov&quot;, 
&quot;Dec&quot;) +&apos;,&apos;+ Text(YEAR( End_Date__c )) 
) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Certification Training Dates</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
