    <validationRules>
        <fullName>New_Enrollment_Class_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
     NOT(ISPICKVAL(  Class__r.Status__c  , &quot;Active&quot;)) 
    ,ISNULL(CreatedDate) 
    ,not($Permission.Allow_Training_Data_Changes)
)</errorConditionFormula>
        <errorMessage>Enrollment cannot be created if Class Status is not Active</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_Enrollment_Contact_Flags</fullName>
        <active>true</active>
        <errorConditionFormula>AND
( OR 
 (
    Enrollee_Name__r.Denied_On_Watch_List_checkbox__c  =  True
  , Enrollee_Name__r.Export_Prohibited_checkbox__c = True
  , Enrollee_Name__r.Account.Denied_Company__c = True
  , Enrollee_Name__r.Account.Export_Status__c = &quot;A&quot; 
 )
, not($Permission.Allow_Training_Data_Changes)
)</errorConditionFormula>
        <errorMessage>Enrollment cannot be created if Contact or Account has Denied Flag or Export Prohibited Flag</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Post_Payment_without_Order_Num</fullName>
        <active>true</active>
        <description>Prevents a user from saving an enrollment with the Notify RAC for Post Payment Processing flag checked if there is no Order Number provided (as RAC needs an Order Number in order to process the invoice.) Workfront Story #1736520</description>
        <errorConditionFormula>AND(
	Notify_RAC_for_Post_Payment_Processing__c 
	,ISBLANK(Order_Number__c)
	,NOT($Permission.Allow_Post_Pay_Enrollment_without_Order_Number) 
)</errorConditionFormula>
        <errorDisplayField>Order_Number__c</errorDisplayField>
        <errorMessage>An Order Number must be provided in order to mark an enrollment for post payment processing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_certification_exam_Test_Name</fullName>
        <active>true</active>
        <description>Prevent users to enter the exam test name if the value of certification exam agreement is anything other than Accept</description>
        <errorConditionFormula>AND
   (
      ISCHANGED(Test_Name__c)
     ,NOT(ISBLANK(Test_Name__c))
     ,Format_Of_Class__c = &quot;Exam&quot;
     ,NOT(ISPICKVAL( Certification_Exam_Agreement__c, &quot;Accept&quot;))
     ,not($Permission.Allow_Training_Data_Changes)
   )</errorConditionFormula>
        <errorDisplayField>Test_Name__c</errorDisplayField>
        <errorMessage>Cannot enter test name if certification exam agreement is not accepted</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_enroll_on_inact_ent_train_agr</fullName>
        <active>true</active>
        <description>Prevent enrollments on inactive training agreements</description>
        <errorConditionFormula>AND(ISNEW(), NOT Enterprise_Training_Agreement__r.Is_Active__c,  NOT ISBLANK( Enterprise_Training_Agreement__c ) )</errorConditionFormula>
        <errorDisplayField>Enterprise_Training_Agreement__c</errorDisplayField>
        <errorMessage>Enterprise Training Agreement is inactive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resend_Notifications_flags_Send_Email</fullName>
        <active>true</active>
        <description>The validation rule ensures that the Resend Notifications flags only can be checked when Send Email is true</description>
        <errorConditionFormula>AND( NOT ( Send_Email__c ) , OR( Send_Awaiting_Payment_Notification__c,  Send_Cancelled_Notification__c,  Send_Class_Cancellation_Notification__c,  Send_Completion_Certificate_Notification__c,  Send_Exam_Notification__c,  Send_Registered_AP_Notification__c,  Send_Registered_Notification__c,  Send_Registered_Reminder_Notification__c, Send_Waitlisted_Notification__c  ))</errorConditionFormula>
        <errorDisplayField>Send_Email__c</errorDisplayField>
        <errorMessage>Resend Notifications flags only can be checked when Send Email is checked</errorMessage>
    </validationRules>
