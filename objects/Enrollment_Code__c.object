    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Prevent_Enrollment_Code_more_than_One</fullName>
        <active>true</active>
        <description>This rule prevents Enterprise Training Agreements from having more than 1 active Enrollment Code at a time.</description>
        <errorConditionFormula>AND(
    Enterprise_Training_Agreement__r.Enrollment_Codes_Count__c &gt;= 1
    ,ISPICKVAL(Status__c , &quot;Active&quot;)
    ,OR(
        ISNEW()
        ,TEXT(PRIORVALUE(Status__c)) != &quot;Active&quot;   
    )
)</errorConditionFormula>
        <errorMessage>This Training Agreement already has an active Enrollment Code. Please revoke the existing active Enrollment Code before creating a new active code.</errorMessage>
    </validationRules>
