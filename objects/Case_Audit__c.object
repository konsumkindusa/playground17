if(Account_Name__c = FALSE, 0,1)+ 
if(Phone_Number__c = FALSE, 0,1)+ 
if(Email_Address__c = FALSE,0,1)+ 
if(License_Number_Product_and_Release__c = FALSE,0,1)+ 
if(Substatus__c = FALSE, 0,1)+ 
if(Status__c = FALSE, 0,1)+ 
if(Area__c = FALSE, 0,1)+ 
