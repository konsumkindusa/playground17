<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Mapping of recipient email addresses to the appropriate handler class for new messages</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Module_Handler_Class__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Name of the Apex class that should handle inbound messages for this email address (do not include .cls)</inlineHelpText>
        <label>Module Handler Class</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Module_Name__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Friendly name of the module the email address is mapped to</inlineHelpText>
        <label>Module Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Recipient_Email_Address__c</fullName>
        <description>Duplicated from the Name field so we can enforce uniqueness</description>
        <externalId>false</externalId>
        <inlineHelpText>The email address that the inbound message was sent to (an alias for an Apex Email Service) - copy this address to the Name field as well</inlineHelpText>
        <label>Recipient Email Address</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>UEH Recipient Module Handler</label>
    <visibility>Public</visibility>
</CustomObject>
