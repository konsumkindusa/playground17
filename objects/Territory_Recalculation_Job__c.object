    <validationRules>
        <fullName>Do_not_allow_change_job</fullName>
        <active>true</active>
        <description>Do not allow any changes once the record has been created and saved except MW Integration user</description>
        <errorConditionFormula>AND
  ( NOT (ISNEW()) 
,
   NOT($User.Id = &apos;005i00000014J7w&apos;)
  
, 
	OR(
		AND(
		ISPICKVAL(Status__c,&apos;Submit&apos;)
		,
		NOT(ISPICKVAL(PRIORVALUE(Status__c),&quot;New&quot;))
		)
	,
		AND(
			NOT(ISPICKVAL(Status__c,&apos;Submit&apos;)),
			ISCHANGED(Status__c)
		)
	)   
)</errorConditionFormula>
        <errorMessage>Jobs that have already been submitted cannot be modified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Do_not_allow_select_status_on_new_record</fullName>
        <active>true</active>
        <description>Do not allow users to select other then New or Submitted status when new record has been created</description>
        <errorConditionFormula>AND(
ISNEW(),
NOT(ISPICKVAL(Status__c,&apos;New&apos;))
,
NOT(ISPICKVAL(Status__c,&apos;Submit&apos;))



)</errorConditionFormula>
        <errorMessage>You can select only New or Submitted job status</errorMessage>
    </validationRules>
