    <fields>
        <fullName>Territory__c</fullName>
        <description>Hyperlink formula to the sales roster page</description>
        <externalId>false</externalId>
        <formula>IF(
    Is_Individual_Account__c
    ,&quot;N/A&quot;
    ,IF(
        NOT(ISBLANK(Salesforce_Territory_Id__c))
        ,HYPERLINK(&quot;/apex/viewterritoryteams?territoryId=&quot;&amp; Salesforce_Territory_Id__c , Territory_Name__c)
        ,IF(
            AND(
                OR(
					AND(
						$Setup.Territory_Settings__c.Territory_Assignment_Lookup_Field__c == &quot;Siebel&quot;
						,NOT(ISBLANK(Siebel_Territory_ID__c))  
						,Siebel_Territory_ID__c != &apos;0-5220&apos; 
					)
					,AND(
						$Setup.Territory_Settings__c.Territory_Assignment_Lookup_Field__c == &quot;Master&quot;
						,NOT(ISBLANK(Master_Territory_ID__c))  
					)				
				)				
                ,ISBLANK(Salesforce_Territory_Id__c)
                ,OR(
                    TA_Attempts__c == 0
                    ,ISBLANK(TA_Attempts__c)
                )                
            )
            ,&quot;Assignment Error&quot;
            ,IF(
                AND(
                    OR(
						AND(
							$Setup.Territory_Settings__c.Territory_Assignment_Lookup_Field__c == &quot;Siebel&quot;
							,NOT(ISBLANK(Siebel_Territory_ID__c))  
							,Siebel_Territory_ID__c != &apos;0-5220&apos;  
						)
						,AND(
							$Setup.Territory_Settings__c.Territory_Assignment_Lookup_Field__c == &quot;Master&quot;
							,NOT(ISBLANK(Master_Territory_ID__c))  
						)				
					)	
                    ,ISBLANK(Salesforce_Territory_Id__c)
                    ,TA_Attempts__c &gt; 0
                    ,TA_Attempts__c &lt;= $Setup.Territory_Settings__c.Max_TA_Attempts__c
                )
                ,&quot;Pending Assignment - Retry&quot;
                ,IF(
                    AND(
                        OR(
							AND(
								$Setup.Territory_Settings__c.Territory_Assignment_Lookup_Field__c == &quot;Siebel&quot;
								,NOT(ISBLANK(Siebel_Territory_ID__c))  
								,Siebel_Territory_ID__c != &apos;0-5220&apos;  
							)
							,AND(
								$Setup.Territory_Settings__c.Territory_Assignment_Lookup_Field__c == &quot;Master&quot;
								,NOT(ISBLANK(Master_Territory_ID__c))  
							)				
						)	
                        ,ISBLANK(Salesforce_Territory_Id__c)
                        ,TA_Attempts__c &gt; $Setup.Territory_Settings__c.Max_TA_Attempts__c
                    )
                    ,&quot;Pending Assignment - Threshold Exceeded&quot;
                    ,&quot;Pending Assignment&quot;                             
                )
            )
        )
    )
)</formula>
