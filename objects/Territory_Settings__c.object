        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Territory_Assignment_Lookup_Field__c</fullName>
        <defaultValue>&quot;Siebel&quot;</defaultValue>
        <description>Switch to use for changing between using Master Territory ID and Siebel Territory ID for looking up corresponding Salesforce Territory2 ID for assignment.  Should not be needed and can be removed after TM solution based on Salesforce/CDS engine goes live - sometime in 2017.</description>
        <externalId>false</externalId>
