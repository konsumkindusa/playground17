        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Generate_Master_Territory_Flag__c</fullName>
        <defaultValue>false</defaultValue>
        <description>The flag controls the process of generation unique Master Territory Id field  of the Territory 2 Object.</description>
        <externalId>false</externalId>
        <inlineHelpText>Flag Controls the generation of the Master Territory Id field of the Territory 2 objects</inlineHelpText>
        <label>Generate Master Territory Flag</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
