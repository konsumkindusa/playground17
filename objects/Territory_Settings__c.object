        <description>The value of the Account territory status(Assignment Error, Pending Retry, etc) to be processed for retry</description>
        <externalId>false</externalId>
        <inlineHelpText>The value of the Account territory status(Assignment Error, Pending Retry, etc) to be processed for retry</inlineHelpText>
        <label>Assignment Status</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Character_Set__c</fullName>
        <description>Character Set that will be used to generate unique Territory Master Id on territory2 object</description>
        <externalId>false</externalId>
        <inlineHelpText>Character Set that will be used to generate unique Territory Master Id on territory2 object</inlineHelpText>
