        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Publish_Territory_Assignment_URL__c</fullName>
        <description>End point url to the CDS web service Publish Assignment Rules. Used when publish rule buttons presses on UI , SFDC posts the call and get back the response from CDS Publish Territory Web service. Contact Salim if url is not working</description>
        <externalId>false</externalId>
        <label>Publish Territory Assignment URL</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
