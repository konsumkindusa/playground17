        <fullName>Assign</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Assign</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)} 

try
{
//for showing loading Image
var errorDiv = document.getElementById(&apos;errorDiv_ep&apos;); 

errorDiv.innerHTML = &quot;&lt;img class=&apos;waitingImage&apos; src=&apos;/img/loading.gif&apos; title=&apos;Please Wait...&apos; /&gt; &lt;span class=&apos;waitingDescription&apos;&gt;Assigning...&lt;/span&gt;&quot;;

errorDiv.style.display = &quot;block&quot;; 




var allRecord = []; //set up empty array


 allRecord.push(&apos;{!Opportunity.Id}&apos;);

//call to utility class to assign opportunities
sforce.apex.execute(&quot;OpportunityAssignmentManager&quot;,&quot;assignOpportunitiesByTerritoryTeamRoles&quot;,{optyIdList:allRecord});

//Refresh page to show new assigned user
location.reload(true);

}
catch (error)
{
alert(error.faultstring);
}</url>
