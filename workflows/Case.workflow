    <rules>
        <fullName>Update SubStatus to In Progress and Area</fullName>
        <actions>
            <name>Update_SubStatus_to_In_Progress</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Default SubStatus to In Progress when a case is created</description>
        <formula>ISPICKVAL( SubStatus__c , &quot;&quot;)</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
