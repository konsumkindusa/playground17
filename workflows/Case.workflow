            <field>Case.Status</field>
            <operation>equals</operation>
            <value>Closed</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.SubStatus__c</field>
            <operation>equals</operation>
            <value>Bug/Enhancement Submitted,Bug Fixed/Enhancement Complete,Closed by Customer,Possible Workaround/Solution,Resolved,Unresolved</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Opt_Out_All_Emails__c</field>
            <operation>notEqual</operation>
            <value>OPTOUT</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Service_Survey_Status__c</field>
            <operation>notEqual</operation>
            <value>Sent,Received,Not Received</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Last_Service_Survey_Sent_Date__c</field>
            <operation>notEqual</operation>
            <value>LAST 90 DAYS</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Employee_Flag__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Service_Survey_Status__c</field>
            <operation>notEqual</operation>
            <value>Received</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Service_Survey_Status__c</field>
            <operation>notEqual</operation>
            <value>Not Received</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Send_Service_Survey__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Last_Service_Survey_Sent_Date__c</field>
            <operation>lessOrEqual</operation>
            <value>TODAY</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Service_Survey_Status__c</field>
            <operation>equals</operation>
            <value>Queued,Sent</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Email</field>
            <operation>notContain</operation>
            <value>@sykes</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.IsVisibleInSelfService</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
