    <fieldUpdates>
        <fullName>Update_Auto_Close_Date_Time</fullName>
        <description>Update Auto Close Date/Time based on criteria of Workflow rule</description>
        <field>Auto_Close_Date_Time__c</field>
        <formula>If(Auto_Close_Days__c = 3, 
  CASE( MOD( DATEVALUE( Now()) - DATE( 1900, 1, 7 ), 7 ),
  0,  NOW ()+ 1 + Auto_Close_Days__c,
  3, Now() + 2 + Auto_Close_Days__c, 
  4,  NOW () + 2 + Auto_Close_Days__c, 
  5,  NOW () + 2 + Auto_Close_Days__c, 
  6,  NOW () + 2 + Auto_Close_Days__c, 
  Now() + Auto_Close_Days__c
  ), 
   If(Auto_Close_Days__c = 4, CASE( MOD( DATEVALUE( Now()) - DATE( 1900, 1, 7 ), 7 ),
    0,  NOW ()+ 1 + Auto_Close_Days__c,
    1, Now() + Auto_Close_Days__c,
    3, Now() + 2 + Auto_Close_Days__c, 
    4,  NOW () + 2 + Auto_Close_Days__c, 
    5,  NOW () + 2 + Auto_Close_Days__c, 
    6,  NOW () + 2 + Auto_Close_Days__c, 
    Now() + 2 + Auto_Close_Days__c
    ),If(Auto_Close_Days__c = 5, CASE( MOD( DATEVALUE( Now()) - DATE( 1900, 1, 7 ), 7 ),
    0,  NOW ()+ 3 + Auto_Close_Days__c,
    1, Now() + 2 + Auto_Close_Days__c,
    3, Now() + 2 + Auto_Close_Days__c, 
    4,  NOW () + 2 + Auto_Close_Days__c, 
    5,  NOW () + 2 + Auto_Close_Days__c, 
    6,  NOW () + 4 + Auto_Close_Days__c, 
    Now() + 2 + Auto_Close_Days__c
    ), null)))</formula>
        <name>Update Auto Close Date/Time</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
