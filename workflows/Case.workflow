            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Auto_Close_Flag</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Follow_up_Date_Time</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Follow_up_Flag</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Update Follow-up &amp; Auto Close flag to True and Follow-up Date &amp; Auto Close Date when Status is Open and SubStatus is Waiting for Customer</description>
        <formula>AND (
NOT (ISPICKVAL ( Origin , &quot;MATLAB Answers&quot;)), 
OR ( OR( ISNEW(), ISCHANGED(SubStatus__c)) &amp;&amp; ISPICKVAL ( SubStatus__c, &quot;Waiting for Customer&quot; ) , ( ISCHANGED ( Follow_up__c ) &amp;&amp; Follow_up__c = True &amp;&amp; ISPICKVAL ( SubStatus__c, &quot;Waiting for Customer&quot; ) ), ( ISCHANGED ( Auto_Close__c ) &amp;&amp; Auto_Close__c = True &amp;&amp; ISPICKVAL (SubStatus__c, &quot;Waiting for Customer&quot; ) ) ), NOT ( ISPICKVAL ( Area__c, &quot;Business Applications&quot; ) || ISPICKVAL ( Area__c, &quot;Grenoble - Advanced Support&quot; ) || ISPICKVAL ( Area__c, &quot;JP - Advanced Support&quot; ) || ISPICKVAL ( Area__c, &quot;Natick - Advanced Support&quot; ) ||ISPICKVAL ( Area__c, &quot;Premier Support - Denso&quot; ) ) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update Follow-up %26 Auto Close %28False%29</fullName>
        <actions>
            <name>Update_Auto_Close_Date_Time_Null</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Auto_Close_Flag_False</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Follow_up_Date_Time_Null</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Follow_up_Flag_False</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 OR 2 OR 3</booleanFilter>
        <criteriaItems>
            <field>Case.SubStatus__c</field>
            <operation>notEqual</operation>
            <value>Waiting for Customer</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Auto_Close__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Area__c</field>
            <operation>equals</operation>
            <value>JP - Advanced Support,Grenoble - Advanced Support,Business Applications,Natick - Advanced Support,Premier Support - Denso</value>
        </criteriaItems>
        <description>Update Follow-up &amp; Auto Close flag to False and Follow-up Date &amp; Auto Close Date when Status is SubStatus is NOT Waiting for Customer</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Update Follow-up %28False%29</fullName>
        <actions>
            <name>Update_Follow_up_Date_Time_Null</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Update Follow-up Date when Follow-up flag is changed to False</description>
        <formula>ISCHANGED( Follow_up__c ) &amp;&amp; Follow_up__c = False</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update License Type to Pirated</fullName>
        <actions>
            <name>Update_License_Type</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Support_Reason__c</field>
            <operation>equals</operation>
