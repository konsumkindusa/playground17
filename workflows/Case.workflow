			,IF(CONTAINS((TEXT(Escalation_to__c)),&quot;Database&quot;)
				,&quot;DB/&quot;&amp;LEFT((TEXT(Escalation_to__c)),2)
				,IF(CONTAINS((TEXT(Escalation_to__c)),&quot;Connections&quot;)
					,&quot;CNX/&quot;&amp;LEFT((TEXT(Escalation_to__c)),2)
					,IF(CONTAINS((TEXT(Escalation_to__c)),&quot;Exchange&quot;)
						,&quot;NC/&quot;&amp;LEFT((TEXT(Escalation_to__c)),2)
						,IF(CONTAINS((TEXT(Escalation_to__c)),&quot;Advanced&quot;)
							,&quot;Adv/&quot;&amp;LEFT((TEXT(Escalation_to__c)),2)
							,IF(CONTAINS((TEXT(Escalation_to__c)),&quot;Revenue Accounting&quot;)
								,&quot;RAC/&quot;&amp;LEFT((TEXT(Escalation_to__c)),2)
								,IF(CONTAINS((TEXT(Escalation_to__c)),&quot;Natick - Tax&quot;)
									,&quot;Tax&quot;
									,IF(CONTAINS((TEXT(Escalation_to__c)),&quot;Natick - Export Compliance&quot; )
										,&quot;Ex Comp&quot;
										,IF(CONTAINS((TEXT(Escalation_to__c)),&quot;Natick - Freight Management&quot; )
											,&quot;Fr Mgmt&quot;
											,IF(CONTAINS((TEXT(Escalation_to__c)),&quot;Natick - License Compliance&quot; )
												,&quot;Li Comp&quot;
												,IF(CONTAINS((TEXT(Escalation_to__c)),&quot;Premier&quot;)
													,&quot;PreDen/&quot;&amp;LEFT((TEXT(Escalation_to__c)),2)
													,&quot;blank&quot;
