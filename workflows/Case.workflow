        <actions>
            <name>Service_Survey_Set_Status_to_Queued</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <booleanFilter>( (1 AND 4 AND 14 AND (7 AND 13))  AND ( (2 AND 3)  AND ( (5 AND 10) OR (8 AND 9) AND (6 AND 11))  OR  (12 AND 1 AND (2 AND 3)) ) )</booleanFilter>
        <criteriaItems>
            <field>Case.Area_Short__c</field>
            <operation>equals</operation>
            <value>Customer Support,Technical Support,Install</value>
