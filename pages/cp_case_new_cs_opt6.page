                                    function(result, event){
                                        if (event.status) {
                                            // Callback();
                                        } else if (event.type === 'exception') {
                                            // handle exception
                                        } else {
                                            // handle error
                                        }
                                    }
                                );
                            }
                            //HW 1776003
                            
                            function doHelpful(input, id, title) {
                                if (confirm('Thank you for your feedback. Do you want to close this case?')) {
