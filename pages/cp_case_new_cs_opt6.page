                            function passArticleId(articleId, articleTitle, caseDeflectionId) {
                                //alert(articleId + ' >>>>> ' + caseDeflectionId);
                                Visualforce.remoting.Manager.invokeAction(
                                    '{!$RemoteAction.cp_Case_new_ControllerExtension.passArticleIdRemote}', 
